@article{ WOS:000864892400001,
Author = {Soto, Christopher J. and Napolitano, Christopher and Sewell, Madison
   Nicole and Yoon, Hee Jun Roy and Roberts, Brent},
Title = {Going Beyond Traits: Social, Emotional, and Behavioral Skills Matter for
   Adolescents' Success},
Journal = {SOCIAL PSYCHOLOGICAL AND PERSONALITY SCIENCE},
Year = {2024},
Volume = {15},
Number = {1},
Pages = {33-45},
Month = {JAN},
Abstract = {The present research addresses three key questions about social,
   emotional, and behavioral (SEB) skills. First, how do SEB skills relate
   with the Big Five traits and Collaborative for Academic, Social, and
   Emotional Learning (CASEL) core competencies? Second, how do SEB skills
   relate with consequential outcomes in adolescence? Third, do SEB skills
   provide incremental validity beyond personality traits? Results from a
   diverse sample of high school students (N = 897) indicate that SEB
   skills converge with the Big Five traits and CASEL competencies in
   expected and conceptually meaningful ways. Analyses of self-reported and
   school-reported outcomes extend SEB skills' nomological network by
   showing that they predict academic achievement and engagement,
   occupational interests, social relationships, civic engagement, and
   well-being. Finally, tests of incremental validity indicate that SEB
   skills provide unique information beyond personality traits and that
   this information matters for predicting outcomes during adolescence.
   These findings advance our understanding of the nature, correlates, and
   consequences of SEB skills.},
Type = {Article},
Language = {English},
Affiliation = {Soto, CJ (Corresponding Author), Colby Coll, Dept Psychol, 5550 Mayflower Hill, Waterville, ME 04901 USA.
   Soto, Christopher J., Colby Coll, Psychol, Waterville, ME 04901 USA.
   Napolitano, Christopher; Sewell, Madison Nicole, Univ Illinois, Educ Psychol, Champaign, IL USA.
   Yoon, Hee Jun Roy; Roberts, Brent, Univ Illinois, Psychol, Champaign, IL USA.
   Roberts, Brent, Univ Tubingen, Tubingen, Germany.},
DOI = {10.1177/19485506221127483},
EarlyAccessDate = {OCT 2022},
Author-Email = {christopher.soto@colby.edu},
Affiliations = {Colby College; University of Illinois System; University of Illinois
   Urbana-Champaign; University of Illinois System; University of Illinois
   Urbana-Champaign; Eberhard Karls University of Tubingen},
Unique-ID = {WOS:000864892400001},
}

@article{ WOS:000858289800001,
Author = {Breil, Simon M. and Mielke, Ina and Ahrens, Helmut and Geldmacher,
   Thomas and Sensmeier, Janina and Marschall, Bernhard and Back, Mitja D.},
Title = {Predicting Actual Social Skill Expression from Personality and Skill
   Self-Concepts},
Journal = {JOURNAL OF INTELLIGENCE},
Year = {2022},
Volume = {10},
Number = {3},
Month = {SEP},
Abstract = {Social skills are of key importance in everyday and work life. However,
   the way in which they are typically assessed via self-report
   questionnaires has one potential downside; self-reports assess
   individuals' global self-concepts, which do not necessarily reflect
   individuals' actual social behaviors. In this research, we aimed to
   investigate how self-concepts assessed via questionnaires relate to
   skill expression assessed via behavioral observations after short
   interpersonal simulations. For this, we used an alternative
   behavior-based skill assessment approach designed to capture expressions
   of predefined social skills. Self- and observer ratings were collected
   to assess three different social skills: agency (i.e., getting ahead in
   social situations), communion (i.e., getting along in social
   situations), and interpersonal resilience (i.e., staying calm in social
   situations). We explored how these skills were related to self-concepts
   by differentiating between a classic personality measure (i.e., Big Five
   Inventory 2; BFI-2) and a novel skill questionnaire (i.e., Behavioral,
   Emotional, and Social Skills Inventory; BESSI). The results (N = 137)
   showed that both personality and skill self-concepts predicted
   self-rated skill expression, with the BESSI showing incremental
   validity. For both personality and skills self-concepts, the
   relationships with observer-rated skill expression were significant for
   agency but not for communion or interpersonal resilience. We discuss
   these results and highlight the theoretical and practical importance of
   differentiating between skill self-concepts and actual skill expression.},
Type = {Article},
Language = {English},
Affiliation = {Breil, SM (Corresponding Author), Univ Munster, Dept Psychol, D-48149 Munster, Germany.
   Breil, Simon M.; Back, Mitja D., Univ Munster, Dept Psychol, D-48149 Munster, Germany.
   Mielke, Ina, Univ Med Ctr Hamburg Eppendorf, Dept Biochem \& Mol Cell Biol, D-20246 Hamburg, Germany.
   Ahrens, Helmut; Geldmacher, Thomas; Sensmeier, Janina; Marschall, Bernhard, Univ Munster, Dept Med, D-48149 Munster, Germany.},
DOI = {10.3390/jintelligence10030048},
Article-Number = {48},
Author-Email = {simon.breil@wwu.de
   i.mielke@uke.de
