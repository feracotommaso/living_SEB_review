DOI = {10.1037/emo0001305},
EarlyAccessDate = {JAN 2024},
Author-Email = {Noah.bokersegal@mail.huji.ac.il},
Affiliations = {Hebrew University of Jerusalem; Hebrew University of Jerusalem},
Unique-ID = {WOS:001137075600001},
}

@article{ WOS:001135937000007,
Author = {Yoon, Hee Jun and Roberts, Brent W. and Sewell, Madison N. and
   Napolitano, Christopher M. and Soto, Christopher J. and Murano, Dana and
   Casillas, Alex},
Title = {Examining SEB skills' incremental validity over personality traits in
   predicting academic achievement},
Journal = {PLOS ONE},
Year = {2024},
Volume = {19},
Number = {1},
Month = {JAN 3},
Abstract = {Personality traits and social, emotional, and behavioral (SEB) skills
   share the same behavioral referents, but whereas traits refer to a
   person's typical or average performance, skills refer to their capacity
   or maximal performance. Given their shared behavioral foundations, an
   important question to address is whether personality traits and SEB
   skills independently predict important outcomes. In this study (N =
   642), we examined whether subscales of the Behavioral, Emotional, and
   Social Skills Inventory (BESSI), a measure of SEB skills, provided
   incremental validity in the prediction of the ACT composite score, an
   important academic outcome for American adolescents, over the Big Five
   personality traits. Consistent with our expectations, on average, SEB
   skills showed stronger associations with ACT achievement scores than
   personality traits. Moreover, SEB skills added incremental validity over
   and above personality traits in predicting ACT achievement scores. The
   findings reinforce the importance of conceptually distinguishing and
   measuring traits and skills.},
Type = {Article},
Language = {English},
Affiliation = {Yoon, HJ (Corresponding Author), Univ Illinois, Dept Psychol, Champaign, IL 61820 USA.
   Yoon, Hee Jun; Roberts, Brent W., Univ Illinois, Dept Psychol, Champaign, IL 61820 USA.
   Sewell, Madison N.; Napolitano, Christopher M., Univ Illinois, Dept Educ Psychol, Champaign, IL USA.
   Soto, Christopher J., Colby Coll, Dept Psychol, Waterville, ME USA.
   Murano, Dana; Casillas, Alex, ACT Inc, Iowa City, IA USA.},
DOI = {10.1371/journal.pone.0296484},
Article-Number = {e0296484},
Author-Email = {yoon14@illinois.edu},
Affiliations = {University of Illinois System; University of Illinois Urbana-Champaign;
