@article{ WOS:001405079400006,
Author = {Sidera, Francesc and Amado, Anna and Andres-Roqueta, Clara and Gorriz
   Plumed, Ana and Perpina, Georgina and Soria-Izquierdo, Eloy and Serrat,
   Elisabet},
Title = {Adaptation and Preliminary Validation of the TRUST for Primary School
   Teachers (TRUST-PS)},
Journal = {PSICOLOGIA EDUCATIVA},
Year = {2025},
Volume = {31},
Number = {1},
Month = {JAN},
Abstract = {Teachers' social and emotional competences (SEC) are important both for
   their well-being and for children's development. Although there are
   self-report instruments that assess teachers' SEC, only the TRUST
   assesses them directly through challenging secondary school classroom
   situations that teachers have to respond to. The aim of this research is
   to adapt it and validate it for primary school teachers, in Catalan
   language. A total of 143 teachers responded to a sociodemographic
   questionnaire, to an adaptation of the TRUST for primary school teachers
   (TRUST-PS), and to an emotional intelligence questionnaire (TEIQue-SF).
   The results revealed that the TRUST-PS has an adequate internal
   consistency for their two subtests: emotion regulation (alpha = .83) and
   relationship management (alpha = .87). Also, a two-factor model showed
   good fit to the data. In conclusion, the TRUST-PS can be used to
   directly assess primary school teachers' SEC and be helpful for
   designing or evaluating teacher trainings.
   Las competencias sociales y emocionales de los maestros y maestras son
   importantes tanto para su bienestar como para el desarrollo de los ni \&
   ntilde;os y ni \& ntilde;as. Aunque existen instrumentos de autoinforme
   que eval \& uacute;an estas competencias en los docentes, solo el TRUST
   las eval \& uacute;a directamente por medio de situaciones desafiantes
   en el aula de secundaria, a las que deben responder los profesores. El
   objetivo de esta investigaci \& oacute;n es adaptarlo y validarlo para
   docentes de primaria en lengua catalana. Un total de 143 docentes
   cumplimentaron un cuestionario sociodemogr \& aacute;fico, una adaptaci
   \& oacute;n del TRUST para docentes de primaria (TRUST-PS) y un
   cuestionario de inteligencia emocional (TEIQue-SF). Los resultados
   revelaron que el TRUST-PS tiene una consistencia interna adecuada para
   sus dos subpruebas: regulaci \& oacute;n de las emociones (alpha = .83)
   y gesti \& oacute;n de las relaciones (alpha = .87). Adem \& aacute;s,
   un modelo de dos factores mostr \& oacute; un buen ajuste a los datos.
   En conclusi \& oacute;n, el TRUST-PS puede utilizarse para evaluar
   directamente las competencias sociales y emocionales de los docentes de
   primaria y ser \& uacute;til para dise \& ntilde;ar o evaluar la formaci
   \& oacute;n dirigida a estos profesionales.},
Type = {Article},
Language = {English},
Affiliation = {Sidera, F (Corresponding Author), Univ Girona, Girona, Spain.
   Sidera, Francesc; Perpina, Georgina; Serrat, Elisabet, Univ Girona, Girona, Spain.
   Amado, Anna, Univ Lleida, Lleida, Spain.
   Andres-Roqueta, Clara; Gorriz Plumed, Ana; Soria-Izquierdo, Eloy, Univ Jaume 1, Castellon de La Plana, Spain.},
DOI = {10.5093/psed2025a8},
Author-Email = {francesc.sidera@udg.edu},
Affiliations = {Universitat de Girona; Universitat de Lleida; Universitat Jaume I},
Unique-ID = {WOS:001405079400006},
